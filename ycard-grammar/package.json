{
  "name": "tree-sitter-ycard",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for yCard",
  "main": "bindings/node",
  "repository": "https://github.com/mieweb/ycard-grammar",
  "license": "MIT",
  "keywords": [
    "ycard",
    "tree-sitter",
    "parser",
    "yabl"
  ],
  "tree-sitter": [
    {
      "scope": "source.ycard",
      "file-types": ["ycard", "yabl"],
      "highlights": "queries/highlights.scm",
      "folds": "queries/folds.scm",
      "indents": "queries/indents.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test"
  }
}